{{#each movieArray}}
  {{#if faveMin}}
    <div class="col-md-3 poster-container" id="{{@key}}">

        <img alt="{{Title}}"
        src='http://img.omdbapi.com/?apikey=7c212437&i={{key}}' width="200px" height="300px">

        <a href="#" class="deleteBtn" key="{{@key}}">
            <span class="glyphicon glyphicon-trash"></span>
        </a>

        <input type="number" id="{{@key}}" class="rating movieRating" data-min="1" data-max="10" data-step="1">
    </div>
  {{/if}}
{{/each}}