<!-- Movie Div *** take out comments and put double handle bars around eaches and visibilities-->
{{#each movieImg}}

 	<div class="col-md-3 poster-container" id="{{key}}"><img alt='{{Title}}' src="http://img.omdbapi.com/?apikey=7c212437&i={{key}}" width="200px" height="300px">
		<input type="number" key ="{{key}}" class="rating" min="1" max="10" step="1">
  		<a href="#" class="deleteBtn" key="{{key}}">
      		<span class="glyphicon glyphicon-trash"></span>
  		</a>
  
   	 <!-- Watched Btns -->
    
    	<button type="button" class="btn watchedBtn">Watched</button>
 	</div>
{{/each}}